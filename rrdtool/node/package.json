{
  "name": "RRD.js",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node --turbo --harmony --max_old_space_size=2000 --gc_interval=100 app.js",
    "pm2-start": "pm2 start npm --name=\"rrd-js\" -- start && pm2 logs rrd-js",
    "pm2-stop": "pm2 stop rrd-js ; pm2 delete rrd-js",
    "pm2-restart": "pm2 restart rrd-js",
    "test": "standard"
  },
  "dependencies": {
    "async": "2.x",
    "body-parser": "1.x",
    "commander": "2.x",
    "compression": "1.x",
    "connect-multiparty": "2.x",
    "cookie-parser": "1.x",
    "cradle": "0.x",
    "errorhandler": "1.x",
    "express": "4.x",
    "helmet": "2.x",
    "log4js": "0.x",
    "method-override": "2.x",
    "moment": "2.x",
    "morgan": "1.x",
    "pm2": "2.x"
  },
  "repository": {},
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "devDependencies": {
    "standard": "*"
  }
}
